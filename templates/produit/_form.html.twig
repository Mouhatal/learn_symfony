{{ form_start(form) }}
{# {{ form_widget(form) }} #}
{{ form_row(form.name) }}
{{ form_row(form.cost) }}

{{ form_row(form.category) }}
<h5>Created at</h5>
<div class="row">
	<div class="col-3">{{ form_widget(form.createdAt) }}</div>
	<div class="col-3 md-form">
		<input placeholder="Selected time" type="time" id="input-limited-range" class="form-control timepicker" name="time">
	</div>
</div>


<button class="btn btn-primary mt-3">{{ button_label|default('Save') }}</button>

{{ form_end(form) }}
